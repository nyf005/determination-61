<div class="row">
  <div class="col-md-6 mx-auto">
    <div class="card card-body">
      <img src="/img/icons8-add-user-male-80.png" alt="logo" class="logo">
      <h3 class="text-center">Nouveau Membre</h3>
      <form action="/members/create" method="POST">
        <div class="form-group">
          <label for="lastName">Nom</label>
          <input type="text" class="form-control" name="lastName" value="">
        </div>
        <div class="form-group">
          <label for="matricule">Matricule</label>
          <input type="number" class="form-control" name="matricule" value="">
        </div>
        <div class="form-group">
          <p>
            <input type="checkbox" name="admin" class="filled-in" id="filled-in-box">
            <label for="filled-in-box">Administrateur</label>
          </p>
        </div>
        <input type="submit" class="btn btn-primary" value="Valider">
      </form>
    </div>
  </div>
</div>